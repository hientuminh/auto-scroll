{
  "manifest_version": 3,
  "name": "My React Chrome Extension",
  "version": "1.0.0",
  "description": "A simple Chrome extension built with React",
  "action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
